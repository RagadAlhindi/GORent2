<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/addbackground"
    tools:context=".rentActivity">

    <TextView
        android:id="@+id/RentTitle"
        android:layout_width="wrap_content"
        android:layout_height="39dp"
        android:layout_marginTop="35dp"
        android:fontFamily="@font/opensauceonebold"
        android:lineSpacingExtra="-1sp"
        android:text="Rent Vehicle"
        android:textColor="@color/white"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="TextSizeCheck" />


    <TextView
        android:id="@+id/RenTitle2"
        android:layout_width="223dp"
        android:layout_height="33dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="28dp"
        android:fontFamily="@font/opensauceonebold"
        android:lineSpacingExtra="-1sp"
        android:text="Fill in the information "
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/RentTitle"
        tools:ignore="TextSizeCheck" />

    <TextView
        android:id="@+id/rentalP"
        android:layout_width="270dp"
        android:layout_height="29dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="144dp"
        android:layout_marginEnd="80dp"
        android:fontFamily="@font/opensauceonesemibold"
        android:text="Rental Period"
        android:textColor="#000000"
        android:textSize="17sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.138"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/period1"
        android:layout_width="310dp"
        android:layout_height="40dp"
        android:layout_alignTop="@+id/rentalP"
        android:layout_marginStart="50dp"
        android:layout_marginTop="184dp"
        android:layout_marginEnd="50dp"
        android:background="@drawable/custom_input"
        android:drawableStart="@drawable/customeplateicon"
        android:drawablePadding="12sp"
        android:ems="10"
        android:hint="From.."
        android:paddingStart="12dp"
        android:paddingEnd="12dp"
        android:textCursorDrawable="@null"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/period2"
        android:layout_width="310dp"
        android:layout_height="40dp"
        android:layout_alignTop="@+id/rentalP"
        android:layout_marginStart="50dp"
        android:layout_marginTop="224dp"
        android:layout_marginEnd="50dp"
        android:background="@drawable/custom_input"
        android:drawableStart="@drawable/customeplateicon"
        android:drawablePadding="12sp"
        android:ems="10"
        android:hint="To.."
        android:paddingStart="12dp"
        android:paddingEnd="12dp"
        android:textCursorDrawable="@null"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/pay"
        android:layout_width="270dp"
        android:layout_height="29dp"
        android:layout_below="@+id/period2"
        android:layout_marginStart="60dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="81dp"
        android:fontFamily="@font/opensauceonesemibold"
        android:text="Payment method"
        android:textColor="#000000"
        android:textSize="17sp"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/period2" />

    <Spinner
        android:id="@+id/method"
        android:layout_width="310dp"
        android:layout_height="40dp"
        android:layout_alignTop="@+id/period2"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="48dp"
        android:background="@drawable/custom_input"
        android:drawablePadding="12sp"
        android:paddingStart="12dp"
        android:paddingEnd="12dp"
        android:textCursorDrawable="@null"
        android:textSize="30dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/period2" />

    <TextView
        android:id="@+id/insurance"
        android:layout_width="270dp"
        android:layout_height="29dp"
        android:layout_below="@+id/period1"
        android:layout_marginStart="60dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="81dp"
        android:fontFamily="@font/opensauceonesemibold"
        android:text="Insurance"
        android:textColor="#000000"
        android:textSize="17sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/method" />


    <EditText
        android:id="@+id/insuType"
        android:layout_width="310dp"
        android:layout_height="40dp"
        android:layout_alignTop="@+id/method"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="64dp"
        android:background="@drawable/custom_input"
        android:drawableStart="@drawable/custommodelicon"
        android:drawablePadding="12sp"
        android:ems="10"
        android:inputType="textCapSentences"
        android:paddingStart="12dp"
        android:paddingEnd="12dp"
        android:textCursorDrawable="@null"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/method" />

    <ImageView
        android:id="@+id/Vehicleimg"
        android:layout_width="136dp"
        android:layout_height="96dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="428dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/VehicleModel"
        android:layout_width="142dp"
        android:layout_height="58dp"
        android:layout_marginTop="432dp"
        android:layout_marginEnd="96dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/VehicleDesc"
        android:layout_width="142dp"
        android:layout_height="58dp"
        android:layout_marginTop="508dp"
        android:layout_marginEnd="28dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/Vehicleplate"
        android:layout_width="101dp"
        android:layout_height="44dp"
        android:layout_marginTop="432dp"
        android:layout_marginEnd="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/VehicleRent"
        android:layout_width="101dp"
        android:layout_height="44dp"
        android:layout_marginTop="584dp"
        android:layout_marginEnd="24dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/VehicleLocation"
        android:layout_width="158dp"
        android:layout_height="66dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="564dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/menuphoto"
        android:layout_width="443dp"
        android:layout_height="101dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/menuaddsvg" />
    <TextView
        android:id="@+id/homeicontext"
        android:layout_width="26dp"
        android:layout_height="13dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="26dp"
        android:fontFamily="@font/opensauceoneregular"
        android:lineSpacingExtra="4sp"
        android:text="HOME"
        android:textColor="#463D82"
        android:textSize="8sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/homeicon"
        tools:ignore="TextSizeCheck" />

    <TextView
        android:id="@+id/basketicontext"
        android:layout_width="34dp"
        android:layout_height="12dp"
        android:layout_marginTop="29dp"
        android:layout_marginEnd="86dp"
        android:fontFamily="@font/opensauceoneregular"
        android:lineSpacingExtra="4sp"
        android:text="RENTED"
        android:textColor="#463D82"
        android:textSize="8sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/basketicon"
        tools:ignore="TextSizeCheck" />

    <TextView
        android:id="@+id/logouticontext"
        android:layout_width="41dp"
        android:layout_height="12dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="24dp"
        android:fontFamily="@font/opensauceoneregular"
        android:lineSpacingExtra="4sp"
        android:text="LOG-OUT"
        android:textColor="@android:color/holo_red_dark"
        android:textSize="8sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/logouticon"
        tools:ignore="TextSizeCheck" />

    <TextView
        android:id="@+id/listicontext"
        android:layout_width="32dp"
        android:layout_height="13dp"
        android:layout_marginStart="84dp"
        android:layout_marginTop="32dp"
        android:fontFamily="@font/opensauceoneregular"
        android:lineSpacingExtra="4sp"
        android:text="OFFERS"
        android:textColor="#463D82"
        android:textSize="8sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/listicon"
        tools:ignore="TextSizeCheck" />

    <ImageView
        android:id="@+id/homeicon"
        android:layout_width="27dp"
        android:layout_height="27dp"
        android:layout_marginStart="28dp"
        android:layout_marginBottom="25dp"
        android:clickable="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/homeicon" />

    <ImageView
        android:id="@+id/listicon"
        android:layout_width="34dp"
        android:layout_height="40dp"
        android:layout_marginStart="28dp"
        android:layout_marginBottom="18dp"
        android:clickable="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/homeicon"
        app:srcCompat="@drawable/listicon" />

    <ImageView
        android:id="@+id/logouticon"
        android:layout_width="31dp"
        android:layout_height="34dp"
        android:layout_marginEnd="29dp"
        android:layout_marginBottom="21dp"
        android:clickable="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/logouticon" />

    <ImageView
        android:id="@+id/basketicon"
        android:layout_width="29dp"
        android:layout_height="31dp"
        android:layout_marginEnd="29dp"
        android:layout_marginBottom="23dp"
        android:clickable="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/logouticon"
        app:srcCompat="@drawable/basketicon" />

    <ImageButton
        android:id="@+id/btnSubmit"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:layout_marginBottom="36dp"
        android:background="@drawable/addcircle"
        android:clickable="true"
        android:src="@drawable/checkicon"
        app:layout_constraintBottom_toBottomOf="@+id/menuphoto"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>