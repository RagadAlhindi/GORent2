<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F6F6F6"
    tools:context=".rentActivity">

    <ScrollView
        android:isScrollContainer="false"
        android:id="@+id/ScrollViewID"
        android:layout_width="fill_parent"
        android:layout_marginTop="0dp"
        android:layout_height="680dp"
        android:animateLayoutChanges="true"
        app:layout_constraintBaseline_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="0dp"
            app:layout_constraintTop_toTopOf="parent"
            android:orientation="vertical">

            <Button
                android:id="@+id/vehicleback"
                android:layout_width="37dp"
                android:layout_height="35dp"
                android:layout_marginStart="10dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/backicon"
                app:backgroundTint="@color/white"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="MissingConstraints,SpeakableTextPresentCheck,TouchTargetSizeCheck" />

            <ImageView
                android:id="@+id/Vehicleimg"
                android:layout_width="fill_parent"
                android:layout_height="340dp"
                android:adjustViewBounds="true"
                android:scaleType="fitXY"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/VehicleModel"
                android:layout_width="340dp"
                android:layout_height="65dp"
                android:layout_below="@+id/Vehicleimg"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:layout_marginStart="0dp"
                android:layout_marginTop="-38dp"
                android:layout_marginEnd="78dp"
                android:background="@drawable/custom_name"
                android:fontFamily="@font/opensauceonebold"
                android:paddingStart="21dp"
                android:paddingTop="5dp"
                android:paddingBottom="10dp"
                android:textColor="@color/black"
                android:textSize="30sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/VehicleDesc1"
                android:layout_width="367dp"
                android:layout_height="wrap_content"
                android:layout_below="@+id/VehicleModel"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:layout_marginStart="12dp"
                android:layout_marginTop="-29dp"
                android:drawableStart="@drawable/line"
                android:lineHeight="19dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/VehicleDesc"
                android:layout_width="367dp"
                android:layout_height="100dp"
                android:layout_below="@+id/VehicleModel"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:layout_marginStart="40dp"
                android:layout_marginTop="-8dp"
                android:layout_marginEnd="25dp"
                android:fontFamily="@font/opensauceoneregular"
                android:lineHeight="19dp"
                android:textColor="@color/black"
                android:textSize="15sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />


            <TextView
                android:id="@+id/VehicleLocation1"
                android:layout_width="198dp"
                android:layout_height="20dp"
                android:layout_below="@+id/VehicleDesc"
                android:layout_alignParentStart="true"
                android:layout_marginStart="15dp"
                android:layout_marginTop="5dp"
                android:layout_marginEnd="0dp"
                android:layout_toStartOf="@+id/VehicleLocation"
                android:drawableStart="@drawable/locationiconfoucsed"
                android:fontFamily="@font/opensauceonebold"
                android:text=" Pick-up Location : "
                android:textColor="@color/black"
                android:textSize="15sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/VehicleLocation"
                android:layout_width="190dp"
                android:layout_height="20dp"
                android:layout_below="@+id/VehicleDesc"
                android:layout_alignParentEnd="true"
                android:layout_marginTop="5dp"
                android:layout_marginEnd="25dp"
                android:fontFamily="@font/opensauceoneregular"
                android:textColor="@color/black"
                android:textSize="15sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/VehicleRent1"
                android:layout_width="135dp"
                android:layout_height="20dp"
                android:layout_below="@+id/Vehicleplate"
                android:layout_alignParentStart="true"
                android:layout_marginStart="15dp"
                android:layout_marginTop="7dp"
                android:drawableStart="@drawable/renticonfoucsed"
                android:fontFamily="@font/opensauceonebold"
                android:text=" Rent Amount : "
                android:textColor="@color/black"
                android:textSize="15sp"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/VehicleRent"
                android:layout_width="250dp"
                android:layout_height="20dp"
                android:layout_below="@+id/Vehicleplate1"
                android:layout_alignParentEnd="true"
                android:layout_marginStart="0dp"
                android:layout_marginTop="5dp"
                android:layout_marginEnd="25dp"
                android:fontFamily="@font/opensauceoneregular"
                android:textColor="@color/black"
                android:layout_toEndOf="@+id/VehicleRent1"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/Vehicleplate1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/VehicleLocation"
                android:layout_alignParentStart="true"
                android:layout_marginStart="15dp"
                android:layout_marginTop="7dp"
                android:drawableStart="@drawable/descriptioniconfoucsed"
                android:rotation="-90"
                android:textSize="15sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/Vehicleplate2"
                android:layout_width="117dp"
                android:layout_height="wrap_content"
                android:layout_below="@+id/VehicleLocation"
                android:layout_alignParentStart="true"
                android:layout_marginStart="38dp"
                android:layout_marginTop="9dp"
                android:fontFamily="@font/opensauceonebold"
                android:text=" Plate Number : "
                android:textColor="@color/black"
                android:textSize="15sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/Vehicleplate"
                android:layout_width="230dp"
                android:layout_height="20dp"
                android:layout_below="@+id/VehicleLocation"
                android:layout_marginStart="0dp"
                android:layout_marginTop="9dp"
                android:layout_marginEnd="0dp"
                android:layout_toEndOf="@+id/Vehicleplate2"
                android:fontFamily="@font/opensauceoneregular"
                android:textColor="@color/black"
                android:textSize="15sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/RenTitle2"
                android:layout_width="357dp"
                android:layout_height="wrap_content"
                android:layout_below="@+id/VehicleRent"
                android:layout_marginStart="20dp"
                android:layout_marginTop="35dp"
                android:fontFamily="@font/opensauceonebold"
                android:lineSpacingExtra="-1sp"
                android:text="Fill in renting details: "
                android:textColor="@color/black"
                android:textSize="20sp"
                app:layout_constraintStart_toStartOf="parent"
                tools:ignore="TextSizeCheck" />


            <TextView
                android:id="@+id/rentalP"
                android:layout_width="270dp"
                android:layout_height="29dp"
                android:layout_below="@+id/RenTitle2"
                android:layout_alignParentStart="true"
                android:layout_marginStart="40dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="80dp"
                android:fontFamily="@font/opensauceonesemibold"
                android:text="Rental Period"
                android:textColor="#000000"
                android:textSize="17sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.083"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <EditText
                android:id="@+id/period1"
                android:layout_width="310dp"
                android:layout_height="40dp"
                android:layout_alignTop="@+id/rentalP"
                android:layout_alignParentStart="true"
                android:layout_marginStart="38dp"
                android:layout_marginTop="25dp"
                android:background="@drawable/custom_input"
                android:drawableStart="@drawable/customeplateicon"
                android:drawablePadding="12sp"
                android:ems="10"
                android:hint="From"
                android:paddingStart="12dp"
                android:paddingEnd="12dp"
                android:textCursorDrawable="@null"
                android:textSize="20dp"
                android:inputType="date"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <EditText
                android:id="@+id/period2"
                android:layout_width="310dp"
                android:layout_height="40dp"
                android:layout_below="@+id/period1"
                android:layout_alignParentStart="true"
                android:layout_marginStart="38dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/custom_input"
                android:drawableStart="@drawable/customeplateicon"
                android:drawablePadding="12sp"
                android:ems="10"
                android:hint="To.."
                android:paddingStart="12dp"
                android:paddingEnd="12dp"
                android:textCursorDrawable="@null"
                android:textSize="20dp"
                android:inputType="date"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/pay"
                android:layout_width="270dp"
                android:layout_height="29dp"
                android:layout_below="@+id/insuType"
                android:layout_alignParentStart="true"
                android:layout_marginStart="40dp"
                android:layout_marginTop="12dp"
                android:layout_marginEnd="81dp"
                android:fontFamily="@font/opensauceonesemibold"
                android:text="Payment Method"
                android:textColor="#000000"
                android:textSize="17sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/period2" />


            <Spinner
                android:id="@+id/method"
                android:layout_width="310dp"
                android:layout_height="40dp"
                android:layout_below="@+id/pay"
                android:layout_alignParentStart="true"
                android:layout_marginStart="38dp"
                android:layout_marginTop="-4dp"
                android:background="@drawable/custom_input"
                android:drawablePadding="12sp"
                android:paddingStart="12dp"
                android:paddingEnd="12dp"
                android:textCursorDrawable="@null"
                android:textSize="30dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/insurance" />

            <TextView
                android:id="@+id/insurance"
                android:layout_width="270dp"
                android:layout_height="29dp"
                android:layout_below="@+id/period2"
                android:layout_alignParentStart="true"
                android:layout_marginStart="40dp"
                android:layout_marginTop="12dp"
                android:layout_marginEnd="81dp"
                android:fontFamily="@font/opensauceonesemibold"
                android:text="Insurance Type"
                android:textColor="#000000"
                android:textSize="17sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/method" />

            <EditText
                android:id="@+id/insuType"
                android:layout_width="310dp"
                android:layout_height="40dp"
                android:layout_below="@+id/insurance"
                android:layout_alignParentStart="true"
                android:layout_marginStart="40dp"
                android:layout_marginTop="-4dp"
                android:background="@drawable/custom_input"
                android:drawableStart="@drawable/custommodelicon"
                android:drawablePadding="12sp"
                android:ems="10"
                android:inputType="textCapSentences"
                android:paddingStart="12dp"
                android:paddingEnd="12dp"
                android:textCursorDrawable="@null"
                android:textSize="20dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/method" />

            <Button
                android:id="@+id/btnSubmit"
                android:layout_width="310dp"
                android:layout_height="44dp"
                android:layout_below="@+id/method"
                android:layout_marginStart="38dp"
                android:layout_marginTop="30dp"
                android:layout_marginBottom="40dp"
                android:background="@drawable/roundedbuton"
                android:fontFamily="@font/opensauceonesemibold"
                android:letterSpacing="0"
                android:text="RENT"
                android:textColor="@color/white"
                android:textSize="20sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.504"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="TouchTargetSizeCheck" />


        </RelativeLayout>
    </ScrollView>



    <ImageView
        android:id="@+id/menuphoto"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/menuaddsvg" />

    <TextView
        android:id="@+id/homeicontext"
        android:layout_width="26dp"
        android:layout_height="13dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="26dp"
        android:fontFamily="@font/opensauceoneregular"
        android:lineSpacingExtra="4sp"
        android:text="HOME"
        android:textColor="#463D82"
        android:textSize="8sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/homeicon"
        tools:ignore="TextSizeCheck" />

    <TextView
        android:id="@+id/basketicontext"
        android:layout_width="34dp"
        android:layout_height="12dp"
        android:layout_marginTop="29dp"
        android:layout_marginEnd="86dp"
        android:fontFamily="@font/opensauceoneregular"
        android:lineSpacingExtra="4sp"
        android:text="RENTED"
        android:textColor="#463D82"
        android:textSize="8sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/basketicon"
        tools:ignore="TextSizeCheck" />

    <TextView
        android:id="@+id/logouticontext"
        android:layout_width="41dp"
        android:layout_height="12dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="24dp"
        android:fontFamily="@font/opensauceoneregular"
        android:lineSpacingExtra="4sp"
        android:text="LOG-OUT"
        android:textColor="@android:color/holo_red_dark"
        android:textSize="8sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/logouticon"
        tools:ignore="TextSizeCheck" />

    <TextView
        android:id="@+id/listicontext"
        android:layout_width="32dp"
        android:layout_height="13dp"
        android:layout_marginStart="84dp"
        android:layout_marginTop="32dp"
        android:fontFamily="@font/opensauceoneregular"
        android:lineSpacingExtra="4sp"
        android:text="OFFERS"
        android:textColor="#463D82"
        android:textSize="8sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/listicon"
        tools:ignore="TextSizeCheck" />

    <ImageView
        android:id="@+id/homeicon"
        android:layout_width="27dp"
        android:layout_height="27dp"
        android:layout_marginStart="28dp"
        android:layout_marginBottom="25dp"
        android:clickable="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/homeicon" />

    <ImageView
        android:id="@+id/listicon"
        android:layout_width="34dp"
        android:layout_height="40dp"
        android:layout_marginStart="28dp"
        android:layout_marginBottom="18dp"
        android:clickable="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/homeicon"
        app:srcCompat="@drawable/listicon" />

    <ImageView
        android:id="@+id/logouticon"
        android:layout_width="31dp"
        android:layout_height="34dp"
        android:layout_marginEnd="29dp"
        android:layout_marginBottom="21dp"
        android:clickable="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/logouticon" />

    <ImageView
        android:id="@+id/basketicon"
        android:layout_width="29dp"
        android:layout_height="31dp"
        android:layout_marginEnd="29dp"
        android:layout_marginBottom="23dp"
        android:clickable="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/logouticon"
        app:srcCompat="@drawable/basketicon" />

    <ImageButton
        android:id="@+id/addicon"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:layout_marginBottom="36dp"
        android:background="@drawable/addcircle"
        android:clickable="true"
        android:src="@drawable/addicon"
        app:layout_constraintBottom_toBottomOf="@+id/menuphoto"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />





</androidx.constraintlayout.widget.ConstraintLayout>